{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "comma-dangle": [
            "error",
            "only-multiline"
        ],
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "no-underscore-dangle": 0,
        "class-methods-use-this": "off",
        "import/prefer-default-export": [
            "off"
        ],
        "no-unused-vars": "off",
        "indent": [
            "error",
            4
        ]
    }
}
